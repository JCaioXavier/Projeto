public class DadosClientes{
    public static List<RequisitosClientes> Clientes(){
        RequisitosClientes caio = new RequisitosClientes();

        caio.id_cliente = 1;
        caio.nome = "Caio";
        caio.cpf = "82334523412";
        caio.endereco = "Rua Teste";
        caio.telefone = "8299335543";

        List<RequisitosClientes> Clientes = new ArrayList<>();
        Clientes.add(caio);

        return Clientes;
    }

}
